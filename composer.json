{
    "name": "yiisoft/cache-redis",
    "type": "library",
    "description": "Yii Caching Library - Redis Handler",
    "keywords": [
        "yii",
        "framework",
        "cache",
        "redis",
        "psr-16"
    ],
    "homepage": "https://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/cache-redis/issues?state=open",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "chat": "https://t.me/yii3en",
        "source": "https://github.com/yiisoft/cache-redis"
    },
    "funding": [
        {
            "type": "opencollective",
            "url": "https://opencollective.com/yiisoft"
        },
        {
            "type": "github",
            "url": "https://github.com/sponsors/yiisoft"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "predis/predis": "^1.1",
        "psr/simple-cache": "^1.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/infection-static-analysis-plugin": "^1.9",
        "spatie/phpunit-watcher": "^1.23",
        "vimeo/psalm": "^4.9"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Cache\\Redis\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Cache\\Redis\\Tests\\": "tests"
        }
    },
    "provide": {
        "psr/simple-cache-implementation": "1.0.0"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit --testdox --no-interaction",
        "test-watch": "phpunit-watcher watch"
    }
}
